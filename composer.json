  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Timgreenwood\\PasswordGenerator\\": "src"
    }
  },
  "authors": [
    {
      "name": "Tim Greenwood",
      "email": "tim@tgmedia.co.uk"
    }
  ],
  "minimum-stability": "dev",
  "require": {
